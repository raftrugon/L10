		<value>/views/security/messages</value>		
		<value>/views/welcome/messages</value>
		<value>/views/user/messages</value>
		<value>/views/systemConfig/messages</value>
		<value>/views/followUp/messages</value>
		<value>/views/newspaper/messages</value>
		<value>/views/article/messages</value>
		<value>/views/customer/messages</value>
		<value>/views/chirp/messages</value>
		<value>/views/subscription/messages</value>
		<value>/views/admin/messages</value>
	</util:list>
</beans>